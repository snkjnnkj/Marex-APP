{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "root": "subpackage",
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "主页"
      }
    },
    {
      "path": "pages/mine/mine",
      "style": {
        "navigationBarTitleText": "个人中心",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/search/search",
      "style": {
        "navigationBarTitleText": "搜索",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/UploadIssue/UploadIssue",
      "style": {
        "navigationBarTitleText": "问题描述"
      }
    },
    {
      "path": "pages/RepairInquiry/RepairInquiry",
      "style": {
        "navigationBarTitleText": "历史维修记录"
      }
    },
    {
      "path": "pages/PersonnelInquiry/PersonnelInquiry",
      "style": {
        "navigationBarTitleText": "高手"
      }
    },
    {
      "path": "pages/Login/Login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/code/code",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/MyData/MyData",
      "style": {
        "navigationBarTitleText": "个人数据",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/name/name",
      "style": {
        "navigationBarTitleText": "name",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/sexPage/sexPage",
      "style": {
        "navigationBarTitleText": "性别",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/setEmailPage/setEmailPage",
      "style": {
        "navigationBarTitleText": "修改邮箱地址",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/setPhonePage/setPhonePage",
      "style": {
        "navigationBarTitleText": "setPhonePage",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/setTeacherPage/setTeacherPage",
      "style": {
        "navigationBarTitleText": "setTeacherPage",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    }
  ],
  "uniIdRouter": {},
  "tabBar": {
    //    "custom": true,
    "borderStyle": "white",
    "color": "#7A7E83",
    "selectedColor": "#0AA9DB",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "height": "60px",
    "fontSize": "14px",
    "iconWidth": "26px",
    "spacing": "1px",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/home1.png",
        "selectedIconPath": "static/home2.png"
      },
      {
        "pagePath": "pages/mine/mine",
        "text": "我的",
        "iconPath": "static/my1.png",
        "selectedIconPath": "static/my2.png"
      },
      {
        "pagePath": "pages/search/search",
        "text": "测试",
        "iconPath": "static/logo.png",
        "selectedIconPath": "static/logo.png"
      }
    ]
  }
}